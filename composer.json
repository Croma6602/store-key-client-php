    "dynamodb",
    "key-value-db"
  ],
  "require": {
    "php": ">=5.6",
    "aws/aws-sdk-php": "^3.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3"
  },
  "license": "Apache2.0",
  "authors": [
    {
      "name": "Wadify",
      "email": "devs@wadify.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
